{
  "environment": "LunarLander-v2",
  "random_seed": 0,
  "number_generations": 20,
  "optimizer": {
    "type": "MU_ES",
    "initial_gene_range": 2,
    "mu": 30,
    "lambda_": 600,
    "mutpb": 0.8
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 10,
    "neuron_activation": "relu",
    "use_bias": true,
    "delta_t": 0.5,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": false
  },
  "episode_runner": {
    "number_fitness_runs": 3
  }
}
