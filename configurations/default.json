{
  "environment": "QbertNoFrameskip-v4",
  "random_seed": 0,
  "number_generations": 330,
  "use_worker_processes": true,
  "optimizer": {
    "type": "MU_ES",
    "hof_size": 10,
    "checkpoint_frequency": 10,
    "initial_gene_range": 2,
    "mate_indpb": 0.3,
    "mutation_Gaussian_sigma_1": 0.5,
    "mutation_Gaussian_sigma_2": 2.0,
    "mutation_Gaussian_indpb_1": 0.1,
    "mutation_Gaussian_indpb_2": 0.007,
    "elitist_ratio": 0.0,
    "tournsize": 40,
    "mu": 10,
    "mu_mixed": 7,
    "mu_mixed_base": 30,
    "mu_novel": 5,
    "extra_from_hof": 4,
    "lambda_": 200,
    "mutpb": 0.8,
    "include_parents_in_next_generation": true,
    "keep_seeds_fixed_during_generation": true,
    "max_recorded_behaviors": 30,
    "mutation_learned": true,
    "distance": "NCD"
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 100,
    "neuron_activation": "relu",
    "neuron_activation_inplace": true,
    "delta_t": 0.5,
    "normalize_input": false,
    "normalize_input_target": 3,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "logarithmic",
    "v_mask_param": 8,
    "w_mask": "logarithmic",
    "w_mask_param": 8,
    "t_mask": "logarithmic",
    "t_mask_param": 8,
    "parameter_perturbations": 0.001
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0,
    "reuse_env": true,
    "behavioral_interval": 1,
    "behavioral_max_length": 300,
    "behavior_from_observation": false
  }
}
