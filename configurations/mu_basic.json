{
  "environment": "Qbert-ram-v0",
  "random_seed": 0,
  "number_generations": 250,
  "optimizer": {
    "type": "MU_ES",
    "population_size": 100,
    "novel_base": 20,
    "checkpoint_frequency": 10,
    "initial_gene_range": 2,
    "mate_indpb": 0.3,
    "mutation_Gaussian_sigma_1": 1.0,
    "mutation_Gaussian_sigma_2": 5.0,
    "mutation_Gaussian_indpb_1": 0.1,
    "mutation_Gaussian_indpb_2": 0.01,
    "elitist_ratio": 0.0,
    "tournsize": 7,
    "mu": 0.2,
    "lambda_": 1.0,
    "mutpb": 0.8,
    "include_parents_in_next_generation": true,
    "keep_seeds_fixed_during_generation": true,
    "max_recorded_behaviors": 50
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 30,
    "delta_t": 0.5,
    "normalize_input": true,
    "normalize_input_target": 2,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 4,
    "parameter_perturbations": 0.005
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "behavioral_interval": 1
  }
}
