{
  "environment": "Ant-v2",
  "random_seed": 0,
  "number_generations": 1000,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 1000,
    "sigma": 0.2,
    "checkpoint_frequency": 20,
    "hof_size": 20,
    "mu": 50,
    "efficiency_weight": 0.0,
    "fix_seed_for_generation": false
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 20,
    "delta_t": 0.5,
    "normalize_input": false,
    "normalize_input_target": 5,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "logarithmic",
    "v_mask_param": 16,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 2,
    "parameter_perturbations": 0.001,
    "neuron_activation": "relu",
    "use_bias": true
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "max_steps_per_run": 0,
    "max_steps_penalty": 0,
    "use_autoencoder": false
  }
}