{
  "environment": "QbertNoFrameskip-v4",
  "random_seed": 0,
  "number_generations": 100,
  "use_worker_processes": true,
  "optimizer": {
    "type": "MU_ES",
    "hof_size": 10,
    "extra_from_hof": 1,
    "checkpoint_frequency": 10,
    "initial_gene_range": 2,
    "tournsize": 0,
    "mu": 30,
    "lambda_": 10000,
    "mutpb": 0.8,
    "efficiency_weight": 0,
    "fix_seed_for_generation": true,
    "strategy_parameter_per_gene": false
  },
  "novelty": {
    "behavioral_interval": 1,
    "behavioral_max_length": -1,
    "behavior_source": "observation",
    "distance": "euclid",
    "max_recorded_behaviors": 100,
    "recorded_behaviors_per_generation": 2,
    "novelty_nearest_k": 50,
    "novelty_weight": 1.0
  },
  "brain": {
    "type": "CTRNN",
    "number_neurons": 30,
    "delta_t": 0.5,
    "normalize_input": false,
    "normalize_input_target": 5,
    "optimize_state_boundaries": "global",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "w_mask": "logarithmic",
    "w_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 2,
    "parameter_perturbations": 0.001,
    "neuron_activation": "relu",
    "neuron_activation_inplace": false,
    "use_bias": true
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": true,
    "number_fitness_runs": 1,
    "reuse_env": true,
    "max_steps_per_run": 1000,
    "max_steps_penalty": 0,
    "use_autoencoder": false,
    "environment_attributes": {
      "screen_size": 16
    }
  }
}
