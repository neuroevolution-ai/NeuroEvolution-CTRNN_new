{
  "environment": "Qbert-ram-v0",
  "neural_network_type": "CTRNN",
  "random_seed": 123,
  "number_generations": 500,
  "trainer_type": "CMA_ES",
  "trainer": {
    "population_size": 150,
    "sigma": 1.0,
    "checkpoint_frequency": 20
  },
  "brain": {
    "number_neurons": 20,
    "delta_t": 0.05,
    "normalize_input": true,
    "normalize_input_target": 10,
    "optimize_state_boundaries": "per_neuron",
    "clipping_range_max": 0.1,
    "clipping_range_min": -0.1,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "w_mask": "logarithmic",
    "w_mask_param": 8,
    "v_mask": "logarithmic",
    "v_mask_param": 4,
    "t_mask": "logarithmic",
    "t_mask_param": 2,
    "parameter_perturbations": 0.001
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1
  }
}
