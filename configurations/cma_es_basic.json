{
  "environment": "BipedalWalker-v3",
  "neural_network_type": "CTRNN",
  "random_seed": 123,
  "number_generations": 11,
  "trainer_type": "CMA_ES",
  "trainer": {
    "population_size": 3,
    "sigma": 1.0,
    "checkpoint_frequency": 20
  },
  "brain": {
    "number_neurons": 5,
    "delta_t": 0.05,
    "normalize_input": false,
    "normalize_input_target": 2,
    "optimize_state_boundaries": "legacy",
    "clipping_range_max": 1.0,
    "clipping_range_min": -1.0,
    "optimize_y0": true,
    "set_principle_diagonal_elements_of_W_negative": true,
    "w_mask": "dense",
    "w_mask_prob": 0.3,
    "v_mask": "dense",
    "v_mask_prob": 0.1,
    "t_mask": "dense",
    "t_mask_prob": 0.3,
    "parameter_perturbations": 0.001
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false,
    "number_fitness_runs": 1
  }
}
