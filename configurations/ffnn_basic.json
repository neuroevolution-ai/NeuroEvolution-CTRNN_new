{
  "environment": "BipedalWalker-v3",
  "random_seed": 123,
  "number_generations": 5,
  "optimizer": {
    "type": "CMA_ES",
    "population_size": 5,
    "sigma": 1.0,
    "mu": 5,
    "fix_seed_for_generation": false
  },
  "brain": {
    "type": "FeedForward_NumPy",
    "use_bias": true,
    "hidden_layers": [[64, 64]],
    "non_linearity": "relu",
    "indirect_encoding": false,
    "cppn_hidden_layers": [[10, 10]]
  },
  "episode_runner": {
    "keep_env_seed_fixed_during_generation": false
  }
}
